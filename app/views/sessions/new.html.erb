<h2>Log In</h2>
<%= form_for :session, url: login_path, :html => {class: "profile_form"} do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username  %>
  <br>

  <%= f.label :password %>
  <%= f.password_field :password %>
  <br>

  <%= f.submit "Login",  class: "wave-effect waves-light btn" %>
<% end  %>

<p>
  Don't have an account?
</p>
<%= link_to "Create Account", new_user_path, class: "wave-effect waves-light btn" %>
